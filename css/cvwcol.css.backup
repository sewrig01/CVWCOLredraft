/* Main Type*/

@media (max-width:480px){

header{
border-bottom:2px solid grey;
background-color:white;
width:100%;
max-width:100%;
}
img.logo{
height: 50px;
float:right;
margin-right:20px;
margin-top:25px;

}
img#facebook{
float:right;
margin-right:7.5px;
margin-top:0;
height:22px;
width=:60px;
border-radius: 0px; 
border-width: 0;
padding-top:45px;
}
img.car{

float:left;
border-radius:15%;
overflow:hidden;
width:100%;
height:100%;
max-width:40%;
margin:10px;

}

footer img {
width:150px;
    height:150px;
    border-radius:50%;
    overflow:hidden;
}

.flexcont {
  display: flex;
  height: 300px; /* Or whatever */
  flex-direction: column;
}

.flexitem {
  flex:none;
  width: 300px;  /* Or whatever */
  height: 20px; /* Or whatever */
  margin: auto;  /* Magic! */
}

.links li{
background-color:white;
list-style:none;

}
.links li a{
display:block;
height:30px;
width:92%;
margin:0;
text-align:center;
padding:5px;
padding-right:0px;
font-size:1.5em;
border-radius: 11px 11px 11px 11px;
-moz-border-radius: 11px 11px 11px 11px;
-webkit-border-radius: 11px 11px 11px 11px;
border: 0px solid #000000;
background:lightblue;
}

h1{
margin:0;
margin-bottom:10px;
max-width:90%;
height:50px;
font-weight:bold;
font-size:1.5em;
vertical-align:top;
font-family:"arial";
color:#435b76;
text-align:center;
padding:5%

}
h2{
clear:both;
margin-left:5%;
Margin-top:10%;
text-align:center;}
body{
background: lightgrey;
max-width:100%;
margin:5%;}



footer{
color:steelblue;
background-color:white;
font-family:impact;
border-top: 2px solid blue;
background-image:linear-gradient(#002,#000);
font-size:.5em;
}
}

